# Outbox Worker Configuration

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/calibr"

# Worker Configuration
OUTBOX_POLL_INTERVAL_MS=5000
OUTBOX_MAX_RETRIES=5
OUTBOX_INITIAL_DELAY_MS=1000
OUTBOX_MAX_DELAY_MS=60000
OUTBOX_BACKOFF_MULTIPLIER=2

# Logging
NODE_ENV=production
LOG_LEVEL=info
SERVICE_NAME=calibr-outbox-worker

# Optional: External integrations
# SLACK_WEBHOOK_URL=
# PAGERDUTY_API_KEY=
